meta {
  name: "Create Product"
  type: http
  seq: 1
}

post {
  url: {{baseUrl}}/store/taxes
  body: json
  auth: inherit
}

body:json {
  {
    "name": "VAT Standard",
    "description": "20% VAT",
    "rate": 20
  }
}

settings {
  encodeUrl: true
  timeout: 0
}

post_response {
  if (res.status === 200 || res.status === 201) {
    bru.setVar("productId", res.body.data.id);
  }
}
