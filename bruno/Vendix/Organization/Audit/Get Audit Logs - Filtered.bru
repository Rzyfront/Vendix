meta {
  name: Get Audit Logs - Filtered
  type: http
  seq: 4
}

get {
  url: {{url}}/organization/admin/audit/logs?limit=5&action=LOGIN
}

tests {
  test("Audit logs retrieved successfully", function() {
    expect(res.status).to.equal(200);
    expect(res.body).to.have.property('success', true);
  });

  test("Respects limit", function() {
    expect(res.body.data).to.be.an('array');
    expect(res.body.data.length).to.be.at.most(5);
  });
}
