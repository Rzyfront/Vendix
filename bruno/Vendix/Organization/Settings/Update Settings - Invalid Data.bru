meta {
  name: Update Settings - Invalid Data
  type: http
  seq: 3
}

put {
  url: http://{{url}}/organization/settings
  body: json
  auth: inherit
}

body:json {
  {
    "settings": "invalid_string_instead_of_object"
  }
}

tests {
  test("Update settings fails with invalid data", function() {
    expect(res.status).to.equal(400);
    expect(res.body).to.have.property('success');
    expect(res.body.success).to.be.false;
  });
}