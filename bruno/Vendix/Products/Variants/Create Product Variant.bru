meta {
  name: "Create Product Variant"
  type: http
  seq: 9
}

post {
  url: http://{{url}}/products/{{productId}}/variants
  body: json
  auth: inherit
}

body:json {
  {
    "product_id": {{productId}},
    "sku": "IPHONE15PRO-BLUE-256GB",
    "price_override": 1099.99,
    "stock_quantity": 25,
    "image_id": 1
  }
}

settings {
  encodeUrl: true
  timeout: 0
}