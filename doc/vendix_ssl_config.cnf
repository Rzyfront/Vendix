[req]
default_bits = 2048
prompt = no
default_md = sha256
distinguished_name = dn
req_extensions = v3_req

[dn]
C=CO
ST=Cundinamarca
L=Bogota
O=Vendix
CN=vendix.com

[v3_req]
keyUsage = keyEncipherment, dataEncipherment
extendedKeyUsage = serverAuth
subjectAltName = @alt_names

[alt_names]
DNS.1 = vendix.com
DNS.2 = www.vendix.com
DNS.3 = api.vendix.com